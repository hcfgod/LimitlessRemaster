{
    "batch": {
        "key1": 100,
        "key2": 200,
        "key3": 300
    },
    "logging": {
        "console_enabled": true,
        "directory": "logs",
        "file_enabled": true,
        "level": "info",
        "max_file_size": 52428800,
        "max_files": 10,
        "pattern": "[%T] [%l] %n: %v"
    },
    "system": {
        "log_directory": "logs",
        "max_threads": 20,
        "temp_directory": "temp",
        "working_directory": "."
    },
    "test": {
        "bool": true,
        "int": 42,
        "range": 50,
        "string": "hello"
    },
    "window": {
        "fullscreen": false,
        "height": 720,
        "resizable": true,
        "title": "Limitless Engine",
        "vsync": false,
        "width": 1280
    }
}